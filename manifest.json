{
  "manifest_version": 3,
  "name": "Auto Chatango User Hider",
  "version": "1.0",
  "description": "Sembunyikan otomatis pesan dari user tertentu di Chatango.",
  "permissions": [],
  "host_permissions": ["*://*.chatango.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.chatango.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ]
}
